{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal",
      "min": 0
    },
    "activity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::activity.activity",
      "inversedBy": "products"
    }
  }
}
